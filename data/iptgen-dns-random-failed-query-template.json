{
  "client": "${client_ip}",
  "server": "${server_ip}:53",
  "sequence": [
    ["for",[0,1000],"i",
      [
        ["foreach.session",["list", ["com", "info", "net", "org", "biz"]],"tld",
          [
            ["env.local.set-payload", {
              "query_name": "${rand.lower_alnum(16)}.${tld}"
            }],
            ["sys.print", "dns.q: ${client_ip} -> ${server_ip}:53: ${query_name}"],
            ["dns.q.a","${query_name}"],
            ["sys.time.drift", 500]
          ]
        ]
      ]
    ]
  ]
}
