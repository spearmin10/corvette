{
  "client": "${client_ip}",
  "server": "${server_ip}:53",
  "sequence": [
    ["for",[0,1000],"i",
      [
        ["foreach.session",["list", ["com", "info", "net", "org", "biz"]],"tld",
          [
            ["sys.print", "dns.q: ${client_ip} -> ${server_ip}:53"],
            ["dns.q.a","${rand.lower_alnum(12)}.${tld}"],
            ["sys.time.drift", 500]
          ]
        ]
      ]
    ]
  ]
}
