        ["", "## Negotiate Protocol Request - SMBv2"],
        ["tcp.send", "hex.raw", [
            "00 00 01 1c fe 53 4d 42  40 00 00 00 00 00 00 00",
            "00 00 00 00 00 00 00 00  00 00 00 00 01 00 00 00",
